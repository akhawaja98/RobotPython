def PromptLandMark6(position):

    #landmark6
    xcoordinaterange0 = random.randint(1157,1163)
    ycoordinaterange0 = random.randint(337,343)
    xcoordinaterange1 = random.randint(1197,1203)
    ycoordinaterange1 = random.randint(297,303)

    print(position)
    
    #position = (w.coords(Robot))
    if ((position[0]) - (xcoordinaterange0) <= 6):
        LosAngeles = Label(root, text= "Welcome to The Empire State Building")
        LosAngeles.place(x=1000, y=340)
        root.after(1500, LosAngeles.destroy)

    elif ((position[1]) - (ycoordinaterange0)) <= 6:
        LosAngeles = Label(root, text= "Welcome to The Empire State Building")
        LosAngeles.place(x=1000, y=340)
        root.after(1500, LosAngeles.destroy)

    elif ((position[2]) - (xcoordinaterange1)) <= 6:
        LosAngeles = Label(root, text= "Welcome to The Empire State Building")
        LosAngeles.place(x=1000, y=340)
        root.after(1500, LosAngeles.destroy)

    elif ((position[3]) - (ycoordinaterange1)) <= 6:
        LosAngeles = Label(root, text= "Welcome to The Empire State Building")
        LosAngeles.place(x=1000, y=340)
        root.after(1500, LosAngeles.destroy)

