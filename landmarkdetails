 while True:

        p = list(w.bbox(Robot))
   
        if (p[0]) >= 1150 and (p[0]) <= 1170:
            PromptLandMark6(w.bbox(Robot))



def PromptLandMark1(position):

    
    #landmark1
    xcoordinaterange0 = random.randint(697,703)
    ycoordinaterange0 = random.randint(397,403)
    xcoordinaterange1 = random.randint(737,743)
    ycoordinaterange1 = random.randint(357,363)

    
    position = (w.coords(Robot))
    if ((position[0]) - (xcoordinaterange0) <= 6):
        LosAngeles = Label(root, text= "Welcome to Los Angeles")
        LosAngeles.place(x=700, y=400)

    elif ((position[1]) - (ycoordinaterange0)) <= 6:
        LosAngeles = Label(root, text= "Welcome to Los Angeles")
        LosAngeles.place(x=700, y=400)

    elif ((position[2]) - (xcoordinaterange1)) <= 6:
        LosAngeles = Label(root, text= "Welcome to Los Angeles")
        LosAngeles.place(x=700, y=400)

    elif ((position[3]) - (ycoordinaterange1)) <= 6:
        LosAngeles = Label(root, text= "Welcome to Los Angeles")
        LosAngeles.place(x=700, y=400)

